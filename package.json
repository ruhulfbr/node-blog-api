{
    "name": "blogs-api",
    "version": "0.1.0",
    "engines": {
        "node": ">=16.0.0"
    },
    "engineStrict": true,
    "private": true,
    "dependencies": {
        "express": "4.19.2",
        "express-async-errors": "3.1.1",
        "joi": "17.13.3",
        "jsonwebtoken": "9.0.2",
        "morgan": "1.10.0",
        "pg": "^8.11.1",
        "sequelize": "6.37.3",
        "shelljs": "0.8.5",
        "http-status-codes": "2.3.0",
        "winston": "3.14.1",
        "express-validator": "7.2.0"
    },
    "main": "src/server.js",
    "scripts": {
        "prestart": "npx sequelize-cli db:create && npx sequelize-cli db:migrate",
        "start": "node .",
        "drop": "npx sequelize-cli db:drop",
        "seed": "npx sequelize-cli db:seed:all",
        "_predev": "npx sequelize-cli db:create && npx sequelize-cli db:migrate",
        "dev": "nodemon .",
        "lint": "eslint --no-inline-config --no-error-on-unmatched-pattern -c .eslintrc.json ."
    },
    "devDependencies": {
        "change-case": "5.4.4",
        "eslint-config-trybe-backend": "2.1.0",
        "frisby": "2.1.3",
        "jest": "29.7.0",
        "jest-each": "29.7.0",
        "nodemon": "3.1.4",
        "sequelize-cli": "6.6.2",
        "sequelize-jest-helpers-trybe": "1.0.0",
        "supertest": "7.0.0",
        "wait-on": "7.2.0"
    }
}
